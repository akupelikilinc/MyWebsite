
@model List<MyWebsite.DAL.Entities.About>

<section class="content">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">DataTable with minimal features & hover style</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>İsim</th>
                                <th>Hakkımda</th>
                                <th>Email</th>
                                <th>Telefon</th>
                                <th>Fotoğraf</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                    <td>@item.AboutID</td>
                                <td>Internet Explorer 4.5</td>
                                <td>Mac OS 8-9</td>
                                <td>-</td>
                                <td>X</td>
                            </tr>
                            }
                        </tbody>
                      
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
           
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
